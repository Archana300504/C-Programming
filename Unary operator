#include<stdio.h>
void main(){ 
  int a=23; 
  printf("a++ = %d\n",a++); 
  printf("a-- = %d\n",a--); 
  printf("++a = %d\n",++a); 
  printf("--a = %d\n",--a); 
}




#include<stdio.h> 
void main() { 
  int a=10,b=10,c=10,d=10;
  printf("a++ = %d\n",a++);
  printf("%d\n",a);  
  printf("++b = %d\n",++b);
  printf("c-- = %d\n",c--);
  printf("--d = %d\n",--d); 
}
